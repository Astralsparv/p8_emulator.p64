--	end
end

function _update()
	if (btnp(0)) page-=1
	if (btnp(1)) page+=1
	if (page<0) page=3
	if (page>3) page=0
	if (btnp(4)) then
		for i=0,63 do
			dset(i,i)
		end
	elseif (btnp(5)) then
		for i=0, 63 do
			dset(i,flr(rnd(255)))
		end
	end
end

function _draw()
	cls()
	color(7)
	local x,y=0,0
	for i=0, 63 do
		local s=tostr(i)
		if (#s==1) s="0"..s
		
		print(s.."="..dget(i),x,y)
		y+=6
		if (y>72) y=0 x+=24+4
	end
	
	print("‚ùé randomise",0,110,7)
	print("üÖæÔ∏è rebuild indexed",0,116,7)
end
